    "def fibo():\n",
    "    first_num = 0\n",
    "    sec_num = 1\n",
    "    while(True):\n",
    "        next_num = first_num + sec_num\n",
    "        yield next_num\n",
    "        first_num,sec_num = sec_num,next_num\n",
    "        \n",
    "r = fibo()\n",
